<image class="background" src="https://s3.ax1x.com/2021/02/01/yel881.png"></image>

<view class="button-image">
</view>

<view wx:if="{{inChina}}">
  <!-- selection0 -->
  
  <view class="button-image">
    <view class="txt" wx:for="{{selectionCathay}}" wx:key="index">
    <view wx:if="{{index%2==0}}">
      <image class="im" style="width: {{imWidth}}rpx; height: {{imHeight}}rpx" src="{{item._icon_url}}" bindtap="{{item.navigator}}" data-id="{{index}}"></image>
    </view>
    <view wx:if="{{index%2==1}}">
      <image class="im1"  style="width: {{imWidth}}rpx; height: {{imHeight}}rpx" src="{{item._icon_url}}" bindtap="{{item.navigator}}" data-id="{{index}}"></image>
    </view>
    </view>
    </view>
    <button class="enterText" bindtap="onNavigate">查看你的报告>>></button>
</view>
